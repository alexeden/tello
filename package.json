{
  "name": "tello",
  "version": "1.0.0",
  "description": "Server for interfacing with a Ryze Tello drone",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeden/tello.git"
  },
  "author": "Alex Eden",
  "license": "ISC",
  "homepage": "https://github.com/alexeden/tello#readme",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "build": "tsc -p src",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "crc": "^3.8.0",
    "ramda": "^0.25.0",
    "rxjs": "^6.3.2"
  },
  "devDependencies": {
    "@types/crc": "^3.4.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.10.1",
    "@types/ramda": "^0.25.38",
    "express": "^4.16.3",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "ts-node --files --pretty -P example/server/tsconfig.json example/server/index.ts",
    "events": {},
    "watch": [
      "src/",
      "example/"
    ]
  }
}
