    "@types/ramda": "^0.25.38",
    "@types/ws": "^6.0.1",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "ws": "^6.0.0"
  },
  "nodemonConfig": {
    "ext": "ts",
    "exec": "npm run test",
    "events": {},
    "watch": [
      "src/"
